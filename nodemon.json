{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": true,
  "execMap": {
    "js": "babel-node"
  },
  "watch": [
    "model/",
    "router/",
    "service/",
    "util/",
    "index.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json"
}